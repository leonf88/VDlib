  sql_db = vdlib_dev
  sql_sock = /var/run/mysqld/mysqld.sock
  sql_query_pre = SET TIME_ZONE = '+0:00'
  sql_query_pre = SET NAMES utf8
  sql_query = SELECT SQL_NO_CACHE `g_regions`.`id` * 3 + 0 AS `id`, `g_regions`.`name_chs` AS `name_chs`, `g_regions`.`name_eng` AS `name_eng`, `g_regions`.`id` AS `sphinx_internal_id`, 'GRegion' AS `sphinx_internal_class`, 0 AS `sphinx_deleted`, `g_regions`.`id` AS `id`, UNIX_TIMESTAMP(`g_regions`.`created_at`) AS `created_at`, UNIX_TIMESTAMP(`g_regions`.`updated_at`) AS `updated_at` FROM `g_regions`  WHERE (`g_regions`.`id` BETWEEN $start AND $end) GROUP BY `g_regions`.`id`, `g_regions`.`name_chs`, `g_regions`.`name_eng`, `g_regions`.`id`, `g_regions`.`id`, `g_regions`.`created_at`, `g_regions`.`updated_at` ORDER BY NULL
  sql_query_range = SELECT IFNULL(MIN(`g_regions`.`id`), 1), IFNULL(MAX(`g_regions`.`id`), 1) FROM `g_regions` 
  sql_attr_uint = sphinx_internal_id
  sql_attr_uint = sphinx_deleted
  sql_attr_uint = id
  sql_attr_timestamp = created_at
  sql_attr_timestamp = updated_at
  sql_attr_string = sphinx_internal_class
  sql_field_string = name_chs
  sql_field_string = name_eng
  sql_query_info = SELECT `g_regions`.* FROM `g_regions`  WHERE (`g_regions`.`id` = ($id - 0) / 3)
}

